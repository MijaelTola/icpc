#include <bits/stdc++.h>
using namespace std;
int main() {
  int T;
  cin>>T;
  while(T--) {
    string s;
    long long k, d;
    for(int i=0; i<5; i++) {
      cin>>s>>k>>d;
      k*=3;
      if(k<d) {
	cout<<s<<" plz uninstall\n";
      }
    }
    for(int i=0; i<5; i++) {
      cin>>s>>k>>d;
      k*=2;
      if(k<d) {
	cout<<s<<" plz uninstall\n";
      }
    }
  }
  return 0;
}
